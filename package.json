{
  "name": "qunit-dom",
  "version": "2.0.0",
  "description": "High Level DOM Assertions for QUnit",
  "keywords": [
    "browser",
    "dom",
    "ember-addon",
    "html",
    "qunit",
    "qunit-plugin",
    "testing"
  ],
  "repository": "https://github.com/simplabs/qunit-dom",
  "license": "MIT",
  "author": "simplabs GmbH",
  "types": "dist/qunit-dom.d.ts",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup -c",
    "changelog": "lerna-changelog",
    "docs": "yarn build && documentation build dist/qunit-dom.js --config documentation.yml -f md -o API.md",
    "lint": "eslint . --cache",
    "prepublish": "rollup -c",
    "release": "release-it",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ember": "ember test",
    "test:watch": "jest --watchAll --notify"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "broccoli-funnel": "^3.0.3",
    "broccoli-merge-trees": "^4.2.0",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-version-checker": "^5.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.5.0",
    "@embroider/compat": "0.46.2",
    "@embroider/core": "0.46.2",
    "@embroider/macros": "0.46.2",
    "@embroider/webpack": "0.46.2",
    "@types/jest": "27.0.2",
    "@types/qunit": "2.11.2",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "documentation": "13.2.5",
    "ember-auto-import": "2.2.2",
    "ember-cli": "3.28.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-htmlbars": "5.7.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.1",
    "ember-try": "1.4.0",
    "eslint": "8.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.2.5",
    "lerna-changelog": "2.2.0",
    "loader.js": "4.7.0",
    "prettier": "2.4.1",
    "qunit": "2.17.2",
    "release-it": "14.11.6",
    "release-it-lerna-changelog": "3.1.0",
    "rollup": "2.58.0",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3",
    "webpack": "5.58.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16.*"
  },
  "changelog": {
    "repo": "simplabs/qunit-dom",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-cli-qunit",
      "ember-qunit"
    ]
  }
}
